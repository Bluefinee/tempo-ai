{
  "name": "tempo-ai-backend",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "build": "tsc --project tsconfig.build.json",
    "type-check": "tsc --noEmit --skipLibCheck",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:api": "bash tests/scripts/test-api.sh",
    "validate": "pnpm run type-check && pnpm run lint && pnpm run format:check",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "prepare": "pnpm run validate && pnpm run build",
    "clean": "rm -rf .wrangler dist",
    "deps:check": "pnpm outdated",
    "deps:update": "pnpm update",
    "quality:check": "pnpm run type-check && pnpm run lint && pnpm run format:check && pnpm run test:coverage",
    "quality:fix": "pnpm run check:fix",
    "security:check": "pnpm audit --audit-level high",
    "security:fix": "pnpm audit --fix",
    "ci:local": "pnpm run quality:check && pnpm run security:check && pnpm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.0",
    "hono": "^4.10.7",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@cloudflare/workers-types": "^4.20251202.0",
    "@stryker-mutator/core": "^8.6.0",
    "@stryker-mutator/typescript-checker": "^8.6.0",
    "@stryker-mutator/vitest-runner": "^8.6.0",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.15",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15",
    "wrangler": "^4.51.0"
  }
}
